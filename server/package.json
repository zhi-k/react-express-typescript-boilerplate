{
  "name": "excellencehubserver",
  "version": "1.0.0",
  "description": "react express server boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all lintfix tsc clean transpile",
    "server": "node -r dotenv/config ./dist/index.js",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "transpile": "babel src --out-dir dist --extensions .ts",
    "clean": "rimraf dist",
    "watch:dev": "nodemon",
    "lint": "eslint ./src",
    "lintfix": "eslint src --ext .ts --fix ",
    "test": "jest",
    "tsc": "tsc"
  },
  "keywords": [
    "express",
    "server"
  ],
  "author": "Chong Zhi Kang",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "express": "^4.17.3"
  }
}
