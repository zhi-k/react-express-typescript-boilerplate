{
  "name": "excellencehubclient",
  "version": "1.0.0",
  "description": "react-express boilerplate",
  "main": "src/index.tsx",
  "scripts": {
    "start": "run-s clean lint tsc prod",
    "start:dev": "run-s clean lint tsc dev watch",
    "clean": "rimraf ./dist ./.cache",
    "build": "parcel build public/index.html --no-source-maps",
    "dev": "cross-env NODE_ENV=development run-s build",
    "prod": "cross-env NODE_ENV=production run-s build",
    "lint": "eslint --ext ts,tsx ./src",
    "tsc": "tsc",
    "watch": "parcel public/index.html --open"
  },
  "keywords": [
    "excellence",
    "hub",
    "react",
    "client"
  ],
  "author": "Chong Zhi Kang",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
