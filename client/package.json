{
  "name": "excellencehubclient",
  "version": "1.0.0",
  "description": "excellence hub react client",
  "main": "index.js",
  "scripts": {
    "start": "run-s clean lint tsc prod",
    "start:dev": "run-s clean lint tsc dev watch",
    "clean": "rimraf ./dist ./.cache",
    "build": "parcel build public/index.html --no-source-maps",
    "dev": "cross-env NODE_ENV=development run-s build",
    "prod": "cross-env NODE_ENV=production run-s build",
    "lint": "eslint --ext ts,tsx ./src",
    "tsc": "tsc",
    "watch": "parcel public/index.html --open"
  },
  "keywords": [
    "excellence",
    "hub",
    "react",
    "client"
  ],
  "author": "Chong Zhi Kang",
  "license": "ISC",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
